[alias]
    ffpull = pull --ff-only
[core]
    excludesfile = /home/sean/.gitignore
    preloadindex = true
[color]
    ui = auto
[diff]
    indentHeuristic = true
[init]
    defaultBranch = main
[log]
    date = local
[merge]
    conflictstyle = diff3
[push]
    default = simple
[receive]
    denyCurrentBranch = warn
